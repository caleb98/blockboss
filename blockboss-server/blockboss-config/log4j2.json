{
    "configuration": {
        "status": "error",
        "name": "BlockBossLoggingConfig",
        "appenders": {
            "Console": {
                "name": "STDOUT",
                "PatternLayout": {
                    "pattern": "[%d{HH:mm:ss}] [BlockBoss.%t-%c{1}/%level]: %m%n"
                }
            }
        },
        "loggers": {
            "root": {
                "level": "DEBUG",
                "AppenderRef": {
                    "ref": "STDOUT"
                }
            }
        }
    }
}